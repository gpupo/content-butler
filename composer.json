{
    "name": "gpupo/content-butler",
    "description": "Content server with PHPCR, Apache Jackrabbit (backend) and Nginx proxy (frontend)",
    "homepage": "https://opensource.gpupo.com/content-butler/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "keywords": ["content", "jackrabbit", "PHPCR"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "gpupo/common-sdk": "^6.4 || dev-main",
        "jackalope/jackalope-jackrabbit": "^1.4",
        "doctrine/phpcr-odm": "1.5.3 || dev-php-8-version-1"
    },
    "autoload": {
        "psr-4": {
            "Gpupo\\ContentButler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gpupo\\ContentButler\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "gpupo/common-dev": "*"
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
